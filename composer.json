{
    "name": "yorss/ensure",
    "description": "Just ensure things are as expected.",
    "license": "MIT",
    "keywords": [
        "just",
        "ensure"
    ],
    "authors": [
        {
            "name": "Kabir Baidhya",
            "email": "kabirbaidhya@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "^0.9.4",
        "phpspec/prophecy": "^1.6",
        "phpunit/phpunit": "^5.3",
        "symfony/var-dumper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yorss\\Ensure\\": "src/"
        },
        "files": [
            "src/Misc/functions.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload --optimize"
        ],
        "post-update-cmd": [
            "composer dump-autoload --optimize"
        ]
    }
}
